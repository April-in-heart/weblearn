#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="$basedir/D:/简历及学习项目文件/大学编程学习文件夹/webpack_study/node_modules/.store/cssesc@3.0.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:$basedir/D:/简历及学习项目文件/大学编程学习文件夹/webpack_study/node_modules/.store/cssesc@3.0.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../cssesc@3.0.0/node_modules/cssesc/bin/cssesc" "$@"
else
  exec node  "$basedir/../../../../../cssesc@3.0.0/node_modules/cssesc/bin/cssesc" "$@"
fi
