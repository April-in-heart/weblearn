#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="$basedir/D:/简历及学习项目文件/大学编程学习文件夹/前端学习文件/molule语法/node_modules/.store/babylon@6.18.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:$basedir/D:/简历及学习项目文件/大学编程学习文件夹/前端学习文件/molule语法/node_modules/.store/babylon@6.18.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../babylon@6.18.0/node_modules/babylon/bin/babylon.js" "$@"
else
  exec node  "$basedir/../../../../../babylon@6.18.0/node_modules/babylon/bin/babylon.js" "$@"
fi
