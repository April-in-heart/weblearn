#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="$basedir/D:/简历及学习项目文件/大学编程学习文件夹/前端学习文件/Vue开发学习/element-plus/node_modules/.store/vue-demi@0.13.11/node_modules"
else
  export NODE_PATH="$NODE_PATH:$basedir/D:/简历及学习项目文件/大学编程学习文件夹/前端学习文件/Vue开发学习/element-plus/node_modules/.store/vue-demi@0.13.11/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../vue-demi@0.13.11/node_modules/vue-demi/bin/vue-demi-fix.js" "$@"
else
  exec node  "$basedir/../../../../../../vue-demi@0.13.11/node_modules/vue-demi/bin/vue-demi-fix.js" "$@"
fi
